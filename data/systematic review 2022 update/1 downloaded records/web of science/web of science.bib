@article{ WOS:000563136400003,
Author = {Perez, William F. and de Almeida, Joao Henrique and Soares, Lucia C. C.
   S. and Wang, Taina F. L. and de Morais, Thais E. D. G. and Mascarenhas,
   V, Andre and de Rose, Julio C.},
Title = {Fearful Faces and the Derived Transfer of Aversive Functions},
Journal = {PSYCHOLOGICAL RECORD},
Year = {2020},
Volume = {70},
Number = {3},
Pages = {387-396},
Month = {SEP},
Abstract = {Studies suggest that fear-related responses might be acquired through
   the facial expressions of other individuals. The present study aimed to
   investigate the derived transfer of aversive functions in equivalence
   classes comprised of facial expressions of fear and happiness. A delayed
   matching-to-sample task established two equivalence classes between
   facial expressions of emotions and nonsense abstract stimuli:
   B1-A1(Fear)-C1-D1; B2-A2(Happiness)-C2-D2. After relational training
