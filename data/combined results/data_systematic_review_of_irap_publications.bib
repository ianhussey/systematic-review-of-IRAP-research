	keywords = {Adult, Appetite Regulation, Attitudes, Body Mass Index, Caloric Restriction, Choice Behavior, Diet, Reducing, Female, Food, Food Preferences, Health, Health Knowledge, Attitudes, Practice, Healthy Diet, Humans, Hunger, Implicit Attitudes, IRAP, Ireland, Logistic Models, Male, Middle Aged, Obese, Obesity, Obesity, Morbid, Patient Compliance, Reaction Time, Self Report, Wanting},
	pages = {41--54},
	file = {McKenna et al. - 2016 - Obesity, food restriction, and implicit attitudes .pdf:files/11522/McKenna et al. - 2016 - Obesity, food restriction, and implicit attitudes .pdf:application/pdf},
