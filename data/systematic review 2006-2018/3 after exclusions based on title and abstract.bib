	file = {Pennington et al. - 2016 - The malleability of stigmatizing attitudes Combin.pdf:/Users/Ian/Zotero/storage/9RHGD64Y/Pennington et al. - 2016 - The malleability of stigmatizing attitudes Combin.pdf:application/pdf},
}

