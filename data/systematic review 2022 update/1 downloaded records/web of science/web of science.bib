@article{ WOS:000498523700003,
Author = {Dunne, Ciara and McEnteggart, Ciara and Harte, Colin and Barnes-Holmes,
   Dermot and Barnes-Holmes, Yvonne},
Title = {Faking a Race IRAP Effect in the Context of Single versus Multiple Label
   Stimuli},
Journal = {INTERNATIONAL JOURNAL OF PSYCHOLOGY AND PSYCHOLOGICAL THERAPY},
Year = {2018},
Volume = {18},
Number = {3},
Pages = {289-300},
Month = {OCT},
Abstract = {In this study, white participants were exposed to a single-label or
   multiple-label racial bias IRAP before and after a faking instruction
   (i.e., two exposures to the IRAP). The faking instruction involved
   asking all participants to imagine that they were a black person when
   completing the second IRAP. The results indicated that participants
   produced evidence of pro-white and anti-black biases both before and
   after receiving the faking instruction. Analyses of variance revealed no
   main or interaction effects for the single-versus multiple-label
   variable, and trial-type specific paired t-tests yielded no significant
   differences between the pre- and post-faking instruction IRAPs. The
   results were consistent with previous racial bias findings using the
   IRAP and supported the conclusion that faking only occurs when
   participants are provided with specific information about the task
   parameters. Implications for faking research, and the impact of
   instructions generally, on the IRAP are discussed.},
