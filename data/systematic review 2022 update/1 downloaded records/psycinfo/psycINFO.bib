	doi = {10.1007/s11031-018-9685-3},
	abstract = {Research on self-determination theory has typically relied on explicit measures when examining the concept of competence need satisfaction. As a result, we know relatively little about competence need satisfaction that arises under conditions of automaticity. Across four studies, we developed and validated implicit measures of competence need satisfaction by drawing on two tasks: a relational variant of the implicit association test (IAT; Study 1, 3, and 4) and the implicit relational assessment procedure (IRAP; Study 2–3). Results across these studies revealed that both implicit measures were either unrelated or moderately related to their explicit counterpart. They were also unrelated to one another. Unlike the IRAP, the IAT was found to be reliable, to display discriminant validity, and to yield meaningful but modest relations with constructs in a nomological network. Together, these results provide modest support for the usefulness of the competence need satisfaction IAT but not of the competence need satisfaction IRAP as an implicit measure of the need for competence. Future research examining the unique predictive value of this IAT is needed, together with research on possible explanations for the low reliability of the IRAP. (PsycINFO Database Record (c) 2020 APA, all rights reserved)},
	number = {5},
	journal = {Motivation and Emotion},
	author = {van der Kaap-Deeder, Jolene and de Houwer, Jan and Hughes, Sean and Spruyt, Adriaan and Vansteenkiste, Maarten},
	month = oct,
	year = {2018},
	note = {Publisher: Springer},
	keywords = {Social Psychology, Test Validity, Test Construction, Psychometrics, Implicit, Implicit Measures, Implicit relational assessment procedure, Competence, Life Satisfaction, Implicit association test, Self-Determination, Self-determination theory},
	pages = {615--637},
}

