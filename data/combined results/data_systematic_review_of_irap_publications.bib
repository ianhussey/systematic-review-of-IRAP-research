	keywords = {Attitude Measurement, career, College Students, Femininity, gender stereotypes, implicit attitudes, Implicit Attitudes, Masculinity, Occupational Attitudes, Occupational Choice, Sciences, Sex Role Attitudes, Sex Roles, STEM, Student Attitudes},
	pages = {121--128},
