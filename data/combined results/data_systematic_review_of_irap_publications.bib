	keywords = {Cognitive Appraisal, excessive responsibility, Fear, Implicit Relational Assessment Procedure, Irrational Beliefs, Measurement, Obsessive Compulsive Disorder, obsessive-compulsive disorder, overestimation of threat, Responsibility, Test Validity, Threat},
	pages = {31--37},
	file = {Nicholson et al. - 2014 - The role of responsibility and threat appraisals i.pdf:files/11530/Nicholson et al. - 2014 - The role of responsibility and threat appraisals i.pdf:application/pdf},
