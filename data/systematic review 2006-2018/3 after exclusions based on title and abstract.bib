	keywords = {Attitude Measures, Implicit Attitudes, Implicit relational assessment procedure, Psychotherapeutic Processes, Psychotherapist, Psychotherapist Attitudes, Psychotherapists, Social distance, Stigma, Substance use disorder, Substance Use Disorder, Test Validity},
