	keywords = {attractiveness bias, Human Sex Differences, implicit bias, IRAP, Measurement, Personality Traits, Physical Attractiveness, RFT},
	pages = {333--351},
	file = {Murphy et al. - 2014 - Implicit Relational Assessment Procedure and attra.pdf:files/11527/Murphy et al. - 2014 - Implicit Relational Assessment Procedure and attra.pdf:application/pdf},
}

@article{maloney_exploring_2016,
	title = {Exploring the behavioral dynamics of the {Implicit} {Relational} {Assessment} {Procedure}: {The} role of relational contextual cues versus relational coherence indicators as response options},
