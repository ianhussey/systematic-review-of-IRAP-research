	keywords = {Dehumanization of women, Human Females, Implicit Attitudes, Implicit Relational Assessment Procedure, Male Attitudes, Relational Frame Theory},
	pages = {291--299},
	file = {Hussey et al. - 2016 - The IRAP is nonrelative but not acontextual Chang.pdf:files/11508/Hussey et al. - 2016 - The IRAP is nonrelative but not acontextual Chang.pdf:application/pdf},
}

