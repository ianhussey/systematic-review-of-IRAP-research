	author = {De Schryver, Maarten and Hussey, Ian and De Neve, Jan and Cartwright, Aoife and Barnes-Holmes, Dermot},
	month = jan,
	year = {2018},
	keywords = {algorithm, Algorithms, alternative scoring, D score, Effect size, Effect Size (Statistical), Implicit measure, Implicit Relational Assessment Procedure, Probabilistic index model, Probability, psychometrics, Psychometrics, Scoring (Testing)},
	pages = {97--103},
	file = {De Schryver et al. - 2018 - The PIIRAP An alternative scoring algorithm for t.pdf:/Users/Ian/Zotero/storage/BHTU6S45/De Schryver et al. - 2018 - The PIIRAP An alternative scoring algorithm for t.pdf:application/pdf},
}

