	journal = {The Psychological Record},
	author = {Vahey, Nigel A. and Barnes-Holmes, Dermot and Barnes-Holmes, Yvonne and Stewart, Ian},
	year = {2009},
	keywords = {Computer Assisted Testing, computerized response-latency-based measure, Implicit Relational Assessment Procedure, implicit self-esteem, IRAP, Response Latency, Self-Esteem, Test Validity, validity},
	pages = {371--388},
	file = {Vahey et al. - 2009 - A first test of the Implicit Relational Assessment.pdf:files/11556/Vahey et al. - 2009 - A first test of the Implicit Relational Assessment.pdf:application/pdf},
}

@article{mcenteggart_pilot_2016,
	title = {A pilot study of the relations within which hearing voices participates: {Towards} a functional distinction between voice hearers and controls},
	volume = {66},
	issn = {0033-2933},
	url = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2016-40563-001&site=ehost-live},
	doi = {10.1007/s40732-016-0196-x},
	abstract = {The current research used the Implicit Relational Assessment Procedure (IRAP) as a preliminary step toward bringing a broad, functional approach to understanding psychosis, by focusing on the specific phenomenon of auditory hallucinations of voices and sounds (often referred to as hearing voices). On this path, we created a taxonomy of some critical features of voice hearing based on the existing literature (i.e., perceived normality of voices, appraisals of self and other people hearing voices, and fear of voices) as a focus of our experimental manipulations. It was our hope that our findings would add to the broad literature that has used explicit measures to study these phenomena, and that the use of an ‘implicit’ measure might assist toward a functional analytic understanding. Three pilot studies were conducted to assess the relations within which hearing voices participates in non-clinical voice hearers (i.e., individuals who hear voices but have no clinical diagnosis or distress) and compared to non-voice hearing control participants. The IRAP effects demonstrated both positive and negative relational responses across the three studies, and these effects varied according to explicit levels of delusional ideation. Furthermore, these IRAP effects also predicted explicit aspects of voice hearing and well-being. The current set of pilot studies demonstrate the utility and precision of the IRAP in this domain, and we propose that this type of experimental analysis may hold potential for future bottom-up functional analyses of voice hearing. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
	number = {4},
	journal = {The Psychological Record},
	author = {McEnteggart, Ciara and Barnes-Holmes, Yvonne and Egger, Jos and Barnes-Holmes, Dermot},
	month = dec,
	year = {2016},
	keywords = {Auditory Hallucinations, Auditory Stimulation, Functional analysis, Functional Analysis, Hearing voices, Implicit relational assessment procedure, Psychosis, Voice},
	pages = {611--626},
	file = {McEnteggart et al. - 2016 - A pilot study of the relations within which hearin.pdf:files/11520/McEnteggart et al. - 2016 - A pilot study of the relations within which hearin.pdf:application/pdf},
}

