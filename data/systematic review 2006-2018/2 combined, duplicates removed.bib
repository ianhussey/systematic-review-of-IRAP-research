@article{song_diminution_2012,
	title = {Diminution of hemoglobin-derived hemorphin: {An} underlying risk factor for cognitive deficit in diabetes},
	volume = {317},
	issn = {0022-510X},
	url = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2012-11610-013&site=ehost-live},
	doi = {10.1016/j.jns.2012.03.001},
	abstract = {Several studies have reported that diabetes could boost the risk of cognitive impairment. Cognitive defect represents an independent complication alongside atherosclerotic cardiovascular disease, nephropathy, neuropathy and retinopathy in diabetic patients. Hemoglobin is known as the precursor of bioactive molecules and not just engaging in gaseous conveyance and exchange anymore. Hemorphin is a set of endogenous neuropeptides originated from enzymatic hydrolysis of hemoglobin. LVV-hemorphin-7 is capable of improving activity-evoked glucose uptake into hippocampal neurons through a GLUT4 mediated approach in vitro. Cathepsin D is considered as a proteolytic enzyme involved in hemorphin production. Hemoglobin hydrolysis products of cathepsin D are quantified. Proteasome, the basic proteolysis machinery, involves in various biologic and pathogenic processes. Hemorphins promote cognitive performance by direct activation of signaling pathway or modulation of insulin-regulated aminopeptidase (IRAP), to preventmetabolismof neurotransmitters. Proteasomal insufficiency induced by hyperglycemia could decrease the release of hemorphins from hydrolytic hemoglobin. Elucidation of the mechanism may therefore provide a novel predictive marker of diabetic brain damage and a promising cognitive enhancing agent. (PsycINFO Database Record (c) 2018 APA, all rights reserved)},
	number = {1-2},
	journal = {Journal of the Neurological Sciences},
	author = {Song, Chang Zheng and Wang, Qing Wei and Song, Chang Cheng},
	month = jun,
	year = {2012},
	keywords = {Humans, Female, Risk Factors, Diabetes, Hypertension, Cognition Disorders, Cognitive Impairment, cognitive deficits, diabetes, hemoglobin, Hemoglobin, hemorphin: risk factors, Diabetes Mellitus, Type 2},
	pages = {157--158},
}

@article{dimaro_anxiety_2014,
	title = {Anxiety and avoidance in psychogenic nonepileptic seizures: {The} role of implicit and explicit anxiety},
	volume = {33},
	issn = {1525-5050},
	url = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2014-13652-017&site=ehost-live},
	doi = {10.1016/j.yebeh.2014.02.016},
	abstract = {This study examined implicit and explicit anxiety in individuals with epilepsy and psychogenic nonepileptic seizures (PNESs) and explored whether these constructs were related to experiential avoidance and seizure frequency. Based on recent psychological models of PNESs, it was hypothesized that nonepileptic seizures would be associated with implicit and explicit anxiety and experiential avoidance. Explicit anxiety was measured by the State-Trait Anxiety Inventory; implicit anxiety was measured by an Implicit Relational Assessment Procedure; and experiential avoidance was measured with the Multidimensional Experiential Avoidance Questionnaire. Although both groups with epilepsy and PNESs scored similarly on implicit measures of anxiety, significant implicit–explicit anxiety discrepancies were only identified in patients with PNESs (p {\textless} .001). In the group with PNESs (but not in the group with epilepsy), explicit anxiety correlated with experiential avoidance (r = .63, p {\textless} .01) and frequency of seizures (r = .67, p {\textless} .01); implicit anxiety correlated with frequency of seizures only (r = .56, p {\textless} .01). Our findings demonstrate the role of implicit anxiety in PNESs and provide additional support for the contribution of explicit anxiety and experiential avoidance to this disorder. (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
	journal = {Epilepsy \& Behavior},
	author = {Dimaro, Lian V. and Dawson, David L. and Roberts, Nicole A. and Brown, Ian and Moghaddam, Nima G. and Reuber, Markus},
	month = apr,
	year = {2014},
	keywords = {Adult, anxiety, Anxiety, avoidance, Avoidance, epilepsy, Epilepsy, Epileptic Seizures, Female, Humans, Male, Middle Aged, nonepileptic seizures, Prospective Studies, Psychophysiologic Disorders, Seizures, Somatoform Disorders, Young Adult},
	pages = {77--86},
	file = {Dimaro et al. - 2014 - Anxiety and avoidance in psychogenic nonepileptic .pdf:/Users/Ian/Zotero/storage/59ZQTGJY/Dimaro et al. - 2014 - Anxiety and avoidance in psychogenic nonepileptic .pdf:application/pdf},
}

@article{otoole_three_2009,
	title = {Three chronometric indices of relational responding as predictors of performance on a {Brief} {Intelligence} {Test}: {The} importance of relational flexibility},
	volume = {59},
	issn = {0033-2933},
	url = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2009-01039-007&site=ehost-live},
	abstract = {Participants completed a before/after and a similar/different relational task, using the Implicit Relational Assessment Procedure (IRAP), and subsequently look the Kaufman Brief Intelligence Test (K-BIT). For each relational task, response latencies were measured first on consistent trials, where participants responded in accordance with preestablished verbal relations, and then on inconsistent trials, where they responded against these relations. A difference-score was calculated by subtracting consistent from inconsistent response latencies. The inconsistent trials and the difference-score provided measures of relational flexibility. Results showed that faster responding on the IRAP and smaller difference- scores predicted higher IQ, These findings suggest that relational flexibility is an important component of intelligence and might therefore be targeted in educational settings. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
	number = {1},
	journal = {The Psychological Record},
	author = {O'Toole, Catriona and Barnes-Holmes, Dermot},
	year = {2009},
	keywords = {cognitive ability, Cognitive Ability, Cognitive Processes, equivalence relation, intelligence, Intelligence, relational flexibility},
	pages = {119--132},
	file = {O'Toole and Barnes-Holmes - 2009 - Three chronometric indices of relational respondin.pdf:/Users/Ian/Zotero/storage/K4PZPTMJ/O'Toole and Barnes-Holmes - 2009 - Three chronometric indices of relational respondin.pdf:application/pdf},
}

