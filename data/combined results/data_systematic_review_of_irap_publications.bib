	keywords = {Implicit Relational Assessment Procedure, Natural Language, Natural Language IRAP, Performance, Phrases, RFT, Stimulus Presentation Methods},
	pages = {247--251},
	file = {Kavanagh et al. - 2016 - Using the IRAP to explore natural language stateme.pdf:files/11511/Kavanagh et al. - 2016 - Using the IRAP to explore natural language stateme.pdf:application/pdf},
}

@article{de_schryver_piirap:_2018,
	title = {The {PIIRAP}: {An} alternative scoring algorithm for the {IRAP} using a probabilistic semiparametric effect size measure},
