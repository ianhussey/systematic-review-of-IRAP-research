	keywords = {Cognition, Contextual cues, Cues, Human adults, Relational coherence indicators, Relational frame theory, Response Parameters, Stimulus Parameters},
	pages = {395--403},
	file = {Maloney and Barnes-Holmes - 2016 - Exploring the behavioral dynamics of the implicit .pdf:files/11518/Maloney and Barnes-Holmes - 2016 - Exploring the behavioral dynamics of the implicit .pdf:application/pdf},
}

