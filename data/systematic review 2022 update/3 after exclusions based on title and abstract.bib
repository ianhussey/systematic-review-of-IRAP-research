	number = {3},
	journal = {PSYCHOLOGICAL RECORD},
	author = {Perez, William F. and de Almeida, Joao Henrique and Soares, Lucia C. C. S. and Wang, Taina F. L. and de Morais, Thais E. D. G. and Mascarenhas, V, Andre and de Rose, Julio C.},
