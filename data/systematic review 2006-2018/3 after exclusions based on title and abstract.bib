	author = {McEnteggart, Ciara and Barnes-Holmes, Yvonne and Mellon, Mirte and van Dongen, Linde and Hendriks, Annemieke and Bames-Holmes, Dermot and Egger, Jos and Harte, Colin},
	year = {2017},
