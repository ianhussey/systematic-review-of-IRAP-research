	year = {2015},
	keywords = {Forgiveness, Implicit measure, Individualized IRAP, Negative and positive behaviors, Negative Emotions, Personality Measures, Positive Emotions, Psychometrics, Self-forgiveness, Test Construction, Test Reliability, Test Validity},
