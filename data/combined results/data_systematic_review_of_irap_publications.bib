	author = {Hughes, Sean and Barnes-Holmes, Dermot and Smyth, Sinead},
	month = mar,
	year = {2017},
	keywords = {Collective Behavior, Implicit cognition, Ingroup Outgroup, IRAP, Northern Ireland, Religious Beliefs, Response Bias},
	pages = {97--107},
	file = {Hughes et al. - 2017 - Implicit cross-community biases revisited Evidenc.pdf:files/11503/Hughes et al. - 2017 - Implicit cross-community biases revisited Evidenc.pdf:application/pdf},
}

@article{vahey_first_2009,
	title = {A first test of the {Implicit} {Relational} {Assessment} {Procedure} as a measure of self-esteem: {Irish} prisoner groups and university students},
