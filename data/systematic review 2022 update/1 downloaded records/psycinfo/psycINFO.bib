	keywords = {Implicit Relational Assessment Procedure, Burnout, Acceptance and Commitment Therapy, Psychological Flexibility, Training, Cognitive Bias, Positivism, Acceptance and Commitment Training, Commitment, Medical Education, Medical Students},
	pages = {190--199},
}

