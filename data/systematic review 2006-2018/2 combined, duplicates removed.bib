	issn = {1471-2202},
	url = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2014-50163-001&site=ehost-live},
	doi = {10.1186/1471-2202-9-S2-S14},
	abstract = {The peptides angiotensin IV and LVV-hemorphin 7 were found to enhance memory in a number of memory tasks and reverse the performance deficits in animals with experimentally induced memory loss. These peptides bound specifically to the enzyme insulin-regulated aminopeptidase (IRAP), which is proposed to be the site in the brain that mediates the memory effects of these peptides. However, the mechanism of action is still unknown but may involve inhibition of the aminopeptidase activity of IRAP, since both angiotensin IV and LVV-hemorphin 7 are competitive inhibitors of the enzyme. IRAP also has another functional domain that is thought to regulate the trafficking of the insulin-responsive glucose transporter GLUT4, thereby influencing glucose uptake into cells. Although the exact mechanism by which the peptides enhance memory is yet to be elucidated, IRAP still represents a promising target for the development of a new class of cognitive enhancing agents. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
	number = {Suppl 2},
	journal = {BMC Neuroscience},
	author = {Chai, Siew Yeen and Yeatman, Holly R. and Parker, Michael W. and Ascher, David B. and Thompson, Philip E. and Mulvey, Hayley T. and Albiston, Anthony L.},
	month = dec,
	year = {2008},
	keywords = {Cognition, Humans, Memory, Animals, Angiotensin, angiotensin, Angiotensin II, cognitive enhancers, Cystinyl Aminopeptidase, Enzymes, Hemoglobins, Insulin, insulin-regulated aminopeptidase, Nootropic Agents, Peptide Fragments, peptides},
}

