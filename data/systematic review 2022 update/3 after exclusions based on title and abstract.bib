	file = {Hernández-López et al. - 2019 - Implicit attitudes to female body shape in Spanish.pdf:/Users/Ian/Zotero Data/storage/SRR86VN9/Hernández-López et al. - 2019 - Implicit attitudes to female body shape in Spanish.pdf:application/pdf},
}

