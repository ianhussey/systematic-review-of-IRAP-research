	note = {Publisher: Frontiers Media S.A.},
	keywords = {Metaphor, relational frame theory, power, Implicit Learning, Cognition, implicit relational assessment procedure, Relational Frame Theory, Measurement, Computers, Concepts, embodied cognition, spatial metaphor, Words (Phonetic Units)},
