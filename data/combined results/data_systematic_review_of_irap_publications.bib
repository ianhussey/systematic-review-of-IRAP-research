	keywords = {attitudes, disgust, Experimental Instructions, faking, Faking, IRAP, Measurement, Performance, race, sexual, Stimulus Parameters},
	pages = {632--648},
	file = {Hughes et al. - 2016 - Faking revisited Exerting strategic control over .pdf:files/11504/Hughes et al. - 2016 - Faking revisited Exerting strategic control over .pdf:application/pdf},
}

