@article{rafacz_assessing_2019,
	title = {Assessing the effects of motivative augmentals, pay-for-performance, and implicit verbal responding on cooperation},
	volume = {69},
	issn = {0033-2933},
	url = {https://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2018-58181-001&site=ehost-live},
	doi = {10.1007/s40732-018-0324-x},
	abstract = {Motivative augmentals are rules or statements that temporarily change the effectiveness of a consequence, similar to establishing operations for nonverbal consequences (Hayes, Barnes-Holmes, \& Roche, 2001). Many communications by an organization's leadership may function as such and alter the function of stimuli in the workplace, which in turn may influence employee behaviors (Houmanfar \& Rodrigues, Journal of Applied Radical Behavior Analysis, N1, 22–27, 2012). There is a lack of experimental research regarding this, however, particularly under different organizational pay systems (i.e., financial contingencies), which have been repeatedly shown to influence performance (e.g., Gupta \& Shaw, Compensation and Benefits Review, 30(2), 26–32, 1998; Locke, Feren, McCaleb, Shaw, \& Denny, 1980). The current study sought to compare the two by measuring the effect of motivational statements on individual versus cooperative responding under two different pay-for-performance contingencies in an organizational analogue. Stimuli for the motivational statements were selected utilizing an Implicit Relational Assessment Procedure (IRAP) with one group of participants, and these statements were then tested under piece-rate and profit-share conditions with a second group of participants in a counterbalanced reversal design. Results indicated that the financial contingencies had more of an effect on responding, but that motivational statements influenced behavior as well. In particular, motivational statements affected whether a participant chose to cooperate or behave individually more under financially neutral conditions (i.e., profit share) when there was no financial cost for doing so. As such, organizations can utilize memos or speeches to increase performance, but it is important for the content to align with financial contingencies in order to maximize performance. (PsycINFO Database Record (c) 2019 APA, all rights reserved)},
	number = {1},
	journal = {The Psychological Record},
	author = {Rafacz, Sharlet D. and Houmanfar, Ramona A. and Smith, Gregory S. and Levin, Michael E.},
	month = mar,
	year = {2019},
	note = {Publisher: Springer},
	keywords = {Behavior Analysis, Communication, Consequence, Cooperation, Experimental Methods, Implicit relational assessment procedure, Job Performance, Leadership, Motivation, Motivative augmentals, Pay-for-performance, Relational frame theory, Rules},
	pages = {49--66},
	annote = {Accession Number: 2018-58181-001. Partial author list: First Author \& Affiliation: Rafacz, Sharlet D.; Department of Psychology, California State University, Fresno, Fresno, CA, US. Other Publishers: Kenyon Coll Psychology Dept; Southern Illinois University. Release Date: 20181115. Correction Date: 20190425. Publication Type: Journal (0100), Peer Reviewed Journal (0110). Format Covered: Electronic. Document Type: Journal Article. Language: EnglishMajor Descriptor: Communication; Cooperation; Job Performance; Leadership; Consequence. Minor Descriptor: Behavior Analysis; Experimental Methods; Motivation. Classification: Group \& Interpersonal Processes (3020). Population: Human (10). Age Group: Adulthood (18 yrs \& older) (300). Tests \& Measures: Data Entry Task. Methodology: Empirical Study; Quantitative Study. References Available: Y. Page Count: 18. Issue Publication Date: Mar 15, 2019. Publication History: First Posted Date: Nov 13, 2018. Copyright Statement: Association for Behavior Analysis International. 2018.},
	file = {Rafacz et al. - 2019 - Assessing the effects of motivative augmentals, pa.pdf:files/14892/Rafacz et al. - 2019 - Assessing the effects of motivative augmentals, pa.pdf:application/pdf},
}

@article{hendriks_assessing_2020,
	title = {Assessing deictic relational responding in social anxiety using the {Implicit} {Relational} {Assessment} {Procedure}},
