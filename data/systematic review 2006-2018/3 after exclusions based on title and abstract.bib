@article{scanlon_using_2014,
	series = {Behavioral {Developmental} {Approaches} to {Clinical} {Interventions}},
	title = {Using the implicit relational assessment procedure ({IRAP}) to assess implicit gender bias and self-esteem in typically-developing children and children with {ADHD} and with dyslexia},
