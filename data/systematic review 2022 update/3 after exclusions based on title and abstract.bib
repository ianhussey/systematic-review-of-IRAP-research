	keywords = {Animals, derived relations, Feedback, Humans, Insulin, Learning, Learning Ability, Persistence, persistent rule-following, Reinforcement, Reinforcement, Psychology, relational coherence, Relational Frame Theory, Responses, rule-governed behaviour, Training},
