	note = {Publisher: Wiley-Blackwell Publishing Ltd.},
	keywords = {Training, Behavior Analysis, coherence, Stimulus Parameters, flexibility, derived relations, HDML, rule‐ following, single participant design, Task Analysis},
	pages = {460--480},
}

