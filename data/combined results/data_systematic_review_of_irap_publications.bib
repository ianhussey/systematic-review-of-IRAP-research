	year = {2016},
	keywords = {D-IRAP scores, Implicit Attitudes, Measurement, Performance, Relational Frame Theory, Trial-types},
	pages = {309--321},
	file = {Finn et al. - 2016 - Exploring the behavioral dynamics of the implicit .pdf:files/11498/Finn et al. - 2016 - Exploring the behavioral dynamics of the implicit .pdf:application/pdf},
}

