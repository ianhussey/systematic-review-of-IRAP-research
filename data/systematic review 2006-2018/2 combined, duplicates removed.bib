@article{barnes-holmes_derived_2018,
	title = {Derived {Stimulus} {Relations} and {Their} {Role} in a {Behavior}-{Analytic} {Account} of {Human} {Language} and {Cognition}},
	volume = {41},
	issn = {0738-6729},
	doi = {10.1007/s40614-017-0124-7},
	abstract = {This article describes how the study of derived stimulus relations has provided the basis for a behavior-analytic approach to the study of human language and cognition in purely functional-analytic terms, with a focus on basic rather than applied research. The article begins with a brief history of the early behavior-analytic approach to human language and cognition, focusing on Skinner's (1957) text Verbal Behavior, his subsequent introduction of the concept of instructional control (Skinner, 1966), and Sidman's (1994) seminal research on stimulus equivalence relations. The article then considers how the concept of derived stimulus relations, as conceptualized within relational frame theory (Hayes et al., 2001), allowed researchers to refine and extend the functional approach to language and cognition in multiple ways. Finally, the article considers some recent conceptual and empirical developments that highlight how the concept of derived stimulus relations continues to play a key role in the behavior-analytic study of human language and cognition, particularly implicit cognition. In general, the article aims to provide a particular perspective on how the study of derived stimulus relations has facilitated and enhanced the behavior analysis of human language and cognition, particularly over the past 25-30 years.},
	number = {1},
	journal = {BEHAVIOR ANALYST},
	author = {Barnes-Holmes, Dermot and Finn, Martin and McEnteggart, Ciara and Barnes-Holmes, Yvonne},
	month = jun,
	year = {2018},
	pages = {155--173},
}

@article{leech_training_2018,
	title = {Training and testing for a transformation of fear and avoidance functions using the {Implicit} {Relational} {Assessment} {Procedure}: {The} first study},
	volume = {157},
	issn = {0376-6357},
	doi = {10.1016/j.beproc.2018.08.012},
	abstract = {Experiment 1 aimed to establish “fearful” and “pleasant” functions for arbitrary stimuli (geometric shapes) by relating those stimuli to pictures of spiders and pets using a training version of the Implicit Relational Assessment Procedure (TRAP). The transformation of these functions for the arbitrary stimuli was assessed by exposing participants to a `traditional' version of the TRAP, the Fear-IRAP employed by Leech et al. (2016, 2017). A broadly similar pattern of response biases was recorded for the Fear-IRAP as had been observed in the previously published studies. Experiment 1 thus supported the assumed but untested assumption that the relational context provided by the IRAP may both serve to establish and reveal fear-related response biases in arbitrary stimuli. A second experiment attempted to replicate the effects observed in Experiment 1 but using pictures of `unfamiliar' Australian marsupials as arbitrary stimuli. The pattern of results obtained in Experiment 2 failed to replicate the pattern observed in Experiment 1, or that reported in the previously published studies by Leech et al. Overall, the findings suggest a possibly important boundary condition for the IRAP as a training and/or testing context for establishing fear-related response biases for arbitrary stimuli.},
	journal = {BEHAVIOURAL PROCESSES},
	author = {Leech, Aileen and Bouyrden, Jaber and Bruijsten, Nathalie and Barnes-Holmes, Dermot and McEnteggart, Ciara},
	month = dec,
	year = {2018},
	pages = {24--35},
	file = {Leech et al. - 2018 - Training and testing for a transformation of fear .pdf:/Users/Ian/Zotero/storage/JMVZ2JSA/Leech et al. - 2018 - Training and testing for a transformation of fear .pdf:application/pdf},
}

@article{petrova_response_2012,
	title = {Response letter to the manuscript '{Diminution} of hemoglobin-derived hemorphin: {An} underlying risk factor for cognitive deficit in diabetes' for {Journal} of the {Neurological} {Sciences}},
	volume = {317},
	issn = {0022-510X},
	url = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2012-11610-014&site=ehost-live},
	doi = {10.1016/j.jns.2012.03.002},
	abstract = {Comments on an article by Chang Zheng Song et al. (see record [rid]2012-11610-013[/rid]). Song et al. mention that hemoglobin-derived hemorphin may clarify the link between diabetes and cognitive impairments. Diabetes was associated with a higher risk of incident cognitive impairment in a population with a high prevalence of this disorder. Thus finding a novel predictive marker of diabetic brain damage and new cognitive enhancing agent is very important. Several recent studies display lower concentrations of plasma hemorphin in diabetes patients in comparison with healthy controls. A number of studies have suggested that LVV-hemorphin 7 could play a role in learning and memory by inhibiting insulin-regulated aminopeptidase (IRAP) activity. It has also been suggested that IRAP ligands could enhance spatial memory by potentiating hippocampal neuronal glucose uptake. These results suggest an important role for the hemorphin in learning and memory processes. Although the exact mechanism by which the peptides enhance memory is yet to be elucidated, LVV-hemorphin-7 still represents a promising target for the identification of a new class of cognitive enhancers. (PsycINFO Database Record (c) 2018 APA, all rights reserved)},
	number = {1-2},
	journal = {Journal of the Neurological Sciences},
	author = {Petrova, Marina and Prokopenko, Semen and Pronina, Elena and Mozheyko, Elena},
	month = jun,
	year = {2012},
	keywords = {Risk Factors, Diabetes, Cognitive Impairment, cognitive deficits, diabetes, hemoglobin, Hemoglobin, hemorphin: risk factors},
	pages = {159--159},
}

@article{de_bundel_ang_2009,
